# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
charset = utf-8
end_of_line = lf

[*.nix]
indent_size = 2

[*.lock]
charset = unset
end_of_line = unset
insert_final_newline = unset
trim_trailing_whitespace = unset
indent_style = unset
indent_size = unset

[.envrc]
indent_size = 2

[*.rs]
trim_trailing_whitespace = true
insert_final_newline = true
indent_style = space
indent_size = 4
